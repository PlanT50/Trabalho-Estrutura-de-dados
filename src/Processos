public class Processo {
    int id;
    String nome;
    int prioridade;
    int ciclos_necessarios; // Separar entre 1-Alta 2-MÃ©dia 3-Baixa
    String recurso_necessario;

    public Processo(int id, String nome, int prioridade, int ciclos_necessarios, String recursos necessarios){
        this.id = id;
        this.nome = nome;
        this.prioridade = prioridade;
        this.ciclos_necessarios = ciclos_necessarios;
        this.recurso_necessario = recurso_necessario;
    }

}
